{
  "manifest_version": 3,
  "name": "Cache Cleaner",
  "description": "A simple extension to clear browser cache",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs",
    "browsingData"
  ],
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "src/icons/icon16.png",
      "48": "src/icons/icon48.png",
      "128": "src/icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "icons": {
    "16": "src/icons/icon16.png",
    "48": "src/icons/icon48.png",
    "128": "src/icons/icon128.png"
  }
}